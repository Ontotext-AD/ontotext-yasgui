  ],
  "bugs": {
    "url": "https://github.com/Ontotext-AD/ontotext-yasgui/issues"
  },
  "homepage": "https://github.com/Ontotext-AD/ontotext-yasgui#readme",
  "devDependencies": {
    "cypress": "^11.2.0",
    "typescript": "^4.9.3",
    "sonarqube-scanner": "^2.7.0"
