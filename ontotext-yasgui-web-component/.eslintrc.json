{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "google"
  ],
  "overrides": [
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "ignorePatterns": [
    "yasgui-script.js",
    "**/*.scss",
    "**/*.html",
    "**/*.md",
    "**/*.eot",
    "**/*.svg",
    "**/*.ttf",
    "**/*.woff",
    "src/pages/**"
  ],
  "rules": {
    // Skips unused imports which start witch underscore.
    "@typescript-eslint/no-unused-vars": ["error", { "varsIgnorePattern": "(^_*)", "argsIgnorePattern": "^_" }],
    "@typescript-eslint/ban-ts-comment": "off",
    "@typescript-eslint/no-explicit-any": "off"
  }
}
